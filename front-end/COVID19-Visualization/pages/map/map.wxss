.container{
    padding: 0px;
    width: 100%;
}

.title{
  padding: 10rpx;
  text-align: center;
  font-size: large;
  font-weight: bold;
}

.box{
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  margin:20rpx;
  padding:0rpx;
}

.row{
  width: 90%;
  display:flex;
  justify-content:space-around;
  border: 3rpx solid black;
}

.left{
  width: 50%;
  padding: 10rpx;
  border-right:3rpx solid black;
  text-align: center;
}

.right{
  width: 50%;
  padding: 10rpx;
  text-align: center;
}

.map{
  height: 800rpx;
  width: 100%;
  margin:10rpx;
}

.dataTable{
  width: 90%;
  padding: 20rpx;
}